<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.rafatarrega.aad4a.MainActivity">


    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintEnd_toStartOf="@+id/insertar"
        app:layout_constraintStart_toStartOf="@+id/insertar">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nombre profesor"
            app:layout_constraintBottom_toBottomOf="@+id/nombreProfesor"
            app:layout_constraintStart_toEndOf="@+id/nombreProfesor" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:text="Curso Alumno"
            app:layout_constraintBottom_toBottomOf="@+id/cursoAlumno"
            app:layout_constraintStart_toEndOf="@+id/cursoAlumno" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Ciclo Alumno"
            app:layout_constraintBaseline_toBaselineOf="@+id/cicloAlumno"
            app:layout_constraintStart_toEndOf="@+id/cicloAlumno" />

        <EditText
            android:id="@+id/cursoAlumno"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/nombreAlumno"
            android:layout_alignStart="@+id/nombreAlumno"
            android:layout_below="@+id/nombreAlumno"
            android:ems="10"
            app:layout_constraintStart_toStartOf="@+id/nombreAlumno"
            app:layout_constraintTop_toBottomOf="@+id/nombreAlumno" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginBottom="4dp"
            android:layout_marginStart="16dp"
            android:text="Datos del alumno"
            app:layout_constraintBottom_toTopOf="@+id/nombreAlumno"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/nombreAlumno"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView3"
            android:layout_alignStart="@+id/textView3"
            android:layout_below="@+id/textView3"
            android:layout_marginTop="60dp"
            android:ems="10"
            android:inputType="textAutoCorrect|textPersonName"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/edadAlumno"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/cursoAlumno"
            android:layout_alignStart="@+id/cursoAlumno"
            android:layout_below="@+id/cursoAlumno"
            android:ems="10"
            android:inputType="textPersonName"
            app:layout_constraintStart_toStartOf="@+id/cursoAlumno"
            app:layout_constraintTop_toBottomOf="@+id/cursoAlumno" />

        <EditText
            android:id="@+id/cicloAlumno"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/edadAlumno"
            android:layout_alignStart="@+id/edadAlumno"
            android:layout_below="@+id/edadAlumno"
            android:ems="10"
            android:inputType="textPersonName"
            app:layout_constraintStart_toStartOf="@+id/edadAlumno"
            app:layout_constraintTop_toBottomOf="@+id/edadAlumno" />

        <EditText
            android:id="@+id/notaMedia"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/cicloAlumno"
            android:layout_alignStart="@+id/cicloAlumno"
            android:layout_below="@+id/cicloAlumno"
            android:ems="10"
            android:inputType="textPersonName"
            app:layout_constraintStart_toStartOf="@+id/cicloAlumno"
            app:layout_constraintTop_toBottomOf="@+id/cicloAlumno" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/notaMedia"
            android:layout_alignStart="@+id/notaMedia"
            android:layout_below="@+id/notaMedia"
            android:layout_marginStart="50dp"
            android:text="Datos profesor"
            app:layout_constraintBottom_toTopOf="@+id/nombreProfesor"
            app:layout_constraintStart_toStartOf="@+id/nombreProfesor" />

        <EditText
            android:id="@+id/nombreProfesor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView4"
            android:layout_alignStart="@+id/textView4"
            android:layout_below="@+id/textView4"
            android:layout_marginTop="34dp"
            android:ems="10"
            android:inputType="textPersonName"
            app:layout_constraintEnd_toStartOf="@+id/buscar"
            app:layout_constraintTop_toTopOf="@+id/buscar" />

        <EditText
            android:id="@+id/edadProfesor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/nombreProfesor"
            android:layout_alignStart="@+id/nombreProfesor"
            android:layout_below="@+id/nombreProfesor"
            android:ems="10"
            android:inputType="textPersonName"
            app:layout_constraintStart_toStartOf="@+id/nombreProfesor"
            app:layout_constraintTop_toBottomOf="@+id/nombreProfesor" />

        <EditText
            android:id="@+id/cicloProfesor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/edadProfesor"
            android:layout_alignRight="@+id/edadProfesor"
            android:layout_below="@+id/edadProfesor"
            android:ems="10"
            android:inputType="textPersonName"
            app:layout_constraintStart_toStartOf="@+id/edadProfesor"
            app:layout_constraintTop_toBottomOf="@+id/edadProfesor" />

        <EditText
            android:id="@+id/despacho"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/cicloProfesor"
            android:layout_alignStart="@+id/cicloProfesor"
            android:layout_below="@+id/cicloProfesor"
            android:ems="10"
            android:inputType="textPersonName"
            app:layout_constraintStart_toStartOf="@+id/cicloProfesor"
            app:layout_constraintTop_toBottomOf="@+id/cicloProfesor" />

        <Button
            android:id="@+id/insertar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/actualizar"
            android:layout_alignLeft="@+id/actualizar"
            android:layout_alignStart="@+id/actualizar"
            android:layout_marginTop="16dp"
            android:text="Insertar"
            app:layout_constraintEnd_toStartOf="@+id/actualizar"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/borrar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/actualizar"
            android:layout_alignRight="@+id/actualizar"
            android:layout_below="@+id/edadAlumno"
            android:text="Borrar"
            app:layout_constraintBaseline_toBaselineOf="@+id/buscar"
            app:layout_constraintStart_toEndOf="@+id/buscar" />

        <Button
            android:id="@+id/actualizar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/borrar"
            android:layout_marginStart="67dp"
            android:layout_toEndOf="@+id/cursoAlumno"
            android:layout_toRightOf="@+id/cursoAlumno"
            android:text="actualizar"
            app:layout_constraintBaseline_toBaselineOf="@+id/insertar"
            app:layout_constraintStart_toStartOf="@+id/textView" />

        <Button
            android:id="@+id/buscar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Buscar"
            app:layout_constraintStart_toEndOf="@+id/notaMedia"
            app:layout_constraintTop_toBottomOf="@+id/notaMedia" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nombre alumno"
            app:layout_constraintBaseline_toBaselineOf="@+id/nombreAlumno"
            app:layout_constraintStart_toEndOf="@+id/nombreAlumno" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Edad Alumno"
            app:layout_constraintBaseline_toBaselineOf="@+id/edadAlumno"
            app:layout_constraintStart_toEndOf="@+id/edadAlumno" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Media Alumno"
            app:layout_constraintBaseline_toBaselineOf="@+id/notaMedia"
            app:layout_constraintStart_toEndOf="@+id/notaMedia" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="9dp"
            android:text="Edad profesor"
            app:layout_constraintBottom_toBottomOf="@+id/edadProfesor"
            app:layout_constraintStart_toEndOf="@+id/edadProfesor" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:text="Ciclo profesor"
            app:layout_constraintBottom_toBottomOf="@+id/cicloProfesor"
            app:layout_constraintStart_toEndOf="@+id/cicloProfesor" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Despacho"
            app:layout_constraintBaseline_toBaselineOf="@+id/despacho"
            app:layout_constraintStart_toEndOf="@+id/despacho" />
    </android.support.constraint.ConstraintLayout>

</RelativeLayout>
